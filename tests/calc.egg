(datatype G (I) (A) (B))
(function g* (G G) G)
(function inv (G) G)
(birewrite (g* (g* a b) c) (g* a (g* b c))) ; assoc
(rewrite (g* I a) a) ; idl
(rewrite (g* a I) a) ; idr
(rewrite (g* (inv a) a) I) ; invl
(rewrite (g* a (inv a)) I) ; invr

; A is cyclic of period 4
(rewrite (g* A (g* A (g* A A))) I)

(define A2 (g* A A))
(define A4 (g* A2 A2))
(define A8 (g* A4 A4))


(calc ()
    (g* A4 A4)
    (g* (g* A2 A2) (g* A2 A2))
    (g* A2 (g* A2 (g* A2 A2)))
)

; Note that (calc ((I G) (b G)) ...) will fail because names must be unused
(calc ((a G) (b G))
    (g* (g* b (g* (inv a) a)) (inv b))
    (g* b (inv b))
    I
)


(rule () ((let l rule-proof)))